{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "Movie"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text"
    },
    "release_date": {
      "type": "date"
    },
    "director": {
      "type": "string"
    },
    "actors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::actor.actor",
      "mappedBy": "movies"
    },
    "id_movie": {
      "type": "uid"
    }
  }
}
